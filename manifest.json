{
  "manifest_version": 3,
  "name": "keyby",
  "version": "1.0.0",
  "description": "Chrome extensions that provides shortcut key and hacks",
  "action": {
    "default_popup": "index.html"
  },
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "js": ["src/content.tsx"],
      "matches": ["<all_urls>"]
    }
  ],
  "background": {
    "service_worker": "src/background.js"
  }
}